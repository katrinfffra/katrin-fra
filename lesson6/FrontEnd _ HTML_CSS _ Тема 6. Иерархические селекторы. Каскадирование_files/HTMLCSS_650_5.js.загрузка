var Str="";


Str+="<div class='SExampleHeader'>";
Str+="Стилевой селектор псевдокласса <b>:hover</b> &mdash; соответствует элементу, над которым сейчас находится курсор мыши.<br><br>";
Str+="Стилевой селектор псевдокласса <b>:active</b> &mdash; соответствует элементу, который сейчас активирован (например, ссылка, когда на ней нажата кнопка мыши).<br><br>";
Str+="Стилевой селектор псевдокласса <b>:focus</b> &mdash; соответствует элементу, который сейчас имеет фокус ввода (например, поле ввода текста, в котором сейчас находится текстовый курсор).<br>";
Str+="<br><hr><br>";
Str+="Стилевой селектор псевдокласса <b>:link</b> &mdash; соответствует ещё не посещённым гиперссылкам.<br>";
Str+="Стилевой селектор псевдокласса <b>:visited</b> &mdash; соответствует уже посещённым гиперссылкам.<br>";
Str+="<br><hr><br>";
Str+="Стилевой селектор псевдокласса <b>:target</b> &mdash; соответствует тем элементам, идентификатор (HTML-атрибут id) которых сейчас указан в закладке УРЛа страницы.";
//"<br><i>(в Internet Explorer поддерживается только начиная с версии 9.0).</i><br>";
Str+="</div>";

Str+="<br><br><hr><br>";

Str+="<div class='SExampleHeader'>";
Str+="Стилевой селектор псевдокласса <b>:not(<i>сел</i>)</b> &mdash; соответствует элементам, <b>не&nbsp;подходящим</b> под указанный селектор <i>сел</i>.";
Str+="<br>Селектор <i>сел</i> не может быть любым, есть некоторые ограничения; например, в&nbsp;нём не&nbsp;допускается использование псевдоэлементов (о них ниже).";
Str+="<br>Обычно селектор :not комбинируется с селекторами других видов.";
Str+="</div>";

var Example=
  "<style>$$$"+
  "  %(.S650x )%div:not(.remark) { color: blue }$$$"+
  "</style>$$$"+
  "%(<div class='S650x'>)%"+
  "<div class='header1'>Заголовок 1</div>$$$"+
  "<div class='header2'>Заголовок 2</div>$$$"+
  "<div>текст текст текст</div>$$$"+
  "<div class='info'>информация информация</div>$$$"+
  "<div class='remark'>примечание</div>$$$"+
  "<div class='info'>текст текст текст</div>$$$"+
  "<p>абзац абзац абзац</p>$$$"+
  "%(</div>)%"+
  "";
Str+=ComposeExamples([{code:Example}],{leftcolperc:60,autoquoteattrs:true});

//Str+="<div class='SExampleDescription'>";
//Str+="Селектор псевдокласса :not введён в CSS3.<br>Internet Explorer до версии 9.0 его не поддерживает.";
//Str+="</div>";


document.write(Str);
