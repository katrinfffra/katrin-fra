var Str="";


Str+="<div class='SExampleHeader'>";
Str+="Стилевой селектор псевдокласса <b>:first-of-type</b> &mdash; соответствует тем элементам, которые в своём контейнере является первыми <b>среди элементов того же типа</b>, т.е. того же тега.";
Str+="<br><br>Стилевой селектор псевдокласса <b>:last-of-type</b> &mdash; соответствует тем элементам, которые в своём контейнере является последними <b>среди элементов того же типа</b>, т.е. того же тега.";
Str+="<br><br>Обычно комбинируются с селектором по тегу.";
Str+="</div>";

var Example=
  "<style>$$$"+
  "  %(.S650z )%p:first-of-type { color: red }$$$"+
  "</style>$$$"+
  "%(<div class='S650z'>)%"+
  "<div>$$$"+
  "  <h3>МУРАВЬИНАЯ СЕМЬЯ</h3>$$$"+
  "  <p>Муравьиные семьи могут существовать на протяжении долгого периода времени.</p>$$$"+
  "  <h3>КОРОЛЕВА</h3>$$$"+
  "  <p>Королева может жить до 20 лет.</p>$$$"+
  "  <i>использованная литература...</i>$$$"+
  "</div>"+
  "%(</div>)%"+
  "";
var Example2=
  "<style>$$$"+
  "  %(.S650y )%p:last-of-type { color: red }$$$"+
  "</style>$$$"+
  "%(<div class='S650y'>)%"+
  "<div>$$$"+
  "  <h3>МУРАВЬИНАЯ СЕМЬЯ</h3>$$$"+
  "  <p>Муравьиные семьи могут существовать на протяжении долгого периода времени.</p>$$$"+
  "  <h3>КОРОЛЕВА</h3>$$$"+
  "  <p>Королева может жить до 20 лет.</p>$$$"+
  "  <i>использованная литература...</i>$$$"+
  "<div>"+
  "%(</div>)%"+
  "";
Str+=ComposeExamples([{code:Example},{code:Example2}],{leftcolperc:59});

//Str+="<div class='SExampleDescription'>";
//Str+="Селекторы псевдоклассов :first-of-type и :last-of-type введены в CSS3.<br>Internet Explorer до версии 9.0 их не поддерживает.";
//Str+="</div>";


document.write(Str);
